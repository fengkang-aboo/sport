.wxParse{
    margin: 0 0px !important;
    font-size: 16px !important;
	background: #fff !important;
}
.wxParse .wxParse-p .wxParse-inline{
	color: #666 !important; 
	background: #fff !important; 
}
.wxParse .wxParse-p .wxParse-strong .wxParse-inline{
	color: #666 !important;
	font-size: 16px !important; 
}

.wxParse-inline{
	font-size:16px !important;
	line-height:30px !important;
	color:#666;
}
.wxParse-img{
    overflow: hidden;
	margin: 10rpx 0;
	width: 100%;
}

